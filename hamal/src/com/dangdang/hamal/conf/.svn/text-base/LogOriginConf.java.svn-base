package com.dangdang.hamal.conf;

import java.util.Map;

public class LogOriginConf {

	/**
	 * Log Origin Type: mysql
	 */
	public static final String LOT_MYSQL="mysql";
	/**
	 * Log Origin Type: file
	 */
	public static final String LOT_FILE="file";
	/**
	 * Log Origin Type: mixed
	 */
	public static final String LOT_MIXED="mixed";
	
	protected Map<String,String> params;
	
	public LogOriginConf()
	{
	}
	private String type;
	
	public Map<String, String> getParams() {
		return params;
	}
	public void setParams(Map<String, String> params) {
		this.params = params;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}	
}
